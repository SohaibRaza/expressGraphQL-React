{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "mocha": true,
        "mongo": true
    },
    "rules": {
        "indent": "off",
        "comma-dangle": ["warn", {
            "arrays": "never"
        }],
        "no-unused-vars": ["error", {

        }],
        "linebreak-style": ["off"],
        "react/jsx-filename-extension": [1, {"extensions":[".js", "jsx"]}] //setting for react
    }
}